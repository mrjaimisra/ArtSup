<%= render partial: "layouts/navbar" %>


<div class="container">
  <div class="row profile">
    <%= image_tag current_user.avatar.url(:medium), class: "img-circle pull-left", alt: current_user.name %>
    <h1 class="header"><%= current_user.name %>'s Profile:</h1>

    <h3><%= current_user.email %></h3>

    <div>
      <%= link_to "Edit Profile", profile_edit_path(user: current_user.id), class: "btn btn-info" %>
    </div>
  </div>

  <div class="wish-list">
  </div>
</div>
